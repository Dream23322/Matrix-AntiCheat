        "controller.animation.may_fly": {
            "states": {
                "default": {
                    "transitions": [
                        {
                            "may_fly": "query.can_fly"
                        }
                    ],
                    "on_entry": ["/tag @s remove may_fly", "/tellraw @a[tag=matrix:packetLogger] {\"rawtext\":[{\"translate\":\"json.packet\",\"with\":[\"MAY_FLY\",\"REMOVE\",{\"rawtext\":[{\"selector\":\"@s\"}]}]}]}"]
                },
                "may_fly": {
                    "transitions": [
                        {
                            "default": "!query.can_fly"
                        }
                    ],
                    "on_entry": ["/tag @s add may_fly", "/tellraw @a[tag=matrix:packetLogger] {\"rawtext\":[{\"translate\":\"json.packet\",\"with\":[\"MAY_FLY\",\"ADD\",{\"rawtext\":[{\"selector\":\"@s\"}]}]}]}"]
                }
            }
        },
